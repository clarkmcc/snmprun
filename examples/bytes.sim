var
    date-time: .1.1.1 bytes { year:2, month:1, day:1, hour:1, minute:1, sec:1, centisecs: 1 } 

endvar

run
    date-time.year = 2018
    date-time.month = 11
    date-time.day = 3
    date-time.hour = 12
    date-time.minute = 30
    date-time.sec = 42
    print "date-time = " + strBytes(date-time)

    sleep 60 secs
endrun
