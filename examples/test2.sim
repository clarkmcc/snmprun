//$ ./pc-print-monitor -h localhost
//Printer Data:
//  enterprise_id = 0
//  printer_desc = Toshiba 2555c
//  marker_count = 1042
//  printer_status = 4 (Printing)
//  printer_error = 0xa0 0x10 (Low Paper|Low Toner|Output Near Full) (byte-swap: off)
//  device_status = 2 (Running) 
//  custom error on active job = FALSE

var
  printer-status: 25.3.5.1.1.1 integer [1 = 'other', 2 = 'p-unknown', 3 = 'idle', 4 = 'printing',
                                        5 = 'warmup', ]
  device-status: 25.3.2.1.5.1 integer [1 = 'd-unknown', 2 = 'running', 3 = 'warning', 
                                       4 = 'testing', 5 = 'down' ]
  error-state: 25.3.5.1.2.1 bitset [0 = 'low paper', 1 = 'no paper', 2 = 'low toner',
                                   3 = 'no toner', 4 = 'door open', 5 = 'jammed',
                                   6 = 'offline', 7 = 'service requested', 
                                   8 = 'input tray missing',
                                   9 = 'output tray missing',
                                   10 = 'marker supply missing',
                                   11 = 'output near full',
                                   12 = 'output full',
                                   13 = 'input tray empty',
                                   14 = 'overdue prvent maint',]
  sys-object: 1.2.0 integer
  device-desc1: 25.3.2.1.3.1 string
  marker-count1: 43.10.2.1.4.1.1 integer
endvar
run
    printer-status = 'printing'
    device-status = 'running'
    device-desc1 = "Toshiba 2555c"
    marker-count1 = 1042
    error-state = ['low paper', 'low toner']
    error-state = error-state + ['output near full']

    print "setting print-state to " + strInt(printer-status)
    print "setting device-state to " + strInt(device-status)

    sleep 2 secs
    error-state = error-state - ['low toner']
    marker-count1 = marker-count1 + 1

    sleep 2 secs
    marker-count1 = marker-count1 + 1
    printer-status = 'idle'

    sleep 10 secs
endrun
